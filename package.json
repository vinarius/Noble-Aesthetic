{
  "name": "noble-aesthetic",
  "version": "0.1.0",
  "description": "The custom, full stack noble aesthetic website.",
  "scripts": {
    "aws-token": "npm run ts-node bin/aws-token",
    "build": "run-s clean build:client",
    "build:client": "cd client && npm run build",
    "clean": "cross-env-shell \"rm -rf cdk.out bin/*.js coverage dist lib/*.js models/*.js src/**/*.js stacks/*.js test/*.js config.js\"",
    "cdk": "cdk",
    "deploy": "run-s \"ts-node bin/deploy\"",
    "destroy": "npm run ts-node bin/destroy",
    "diff": "npm run cdk diff",
    "lint": "eslint . --ext .ts",
    "postinstall": "cd client && npm i",
    "syncHostBucket": "npm run ts-node bin/syncHostBucket",
    "synth": "run-p \"ts-node bin/synth\"",
    "test": "jest --coverage --passWithNoTests",
    "ts-node": "ts-node",
    "tsc": "cross-env-shell \"node_modules/.bin/tsc --project tsconfig.build.json\"",
    "watch": "npm run ts-node bin/cdkWatch"
  },
  "devDependencies": {
    "@aws-sdk/client-iam": "^3.43.0",
    "@aws-sdk/client-sts": "^3.43.0",
    "@aws-sdk/credential-provider-ini": "^3.41.0",
    "@types/aws-lambda": "^8.10.88",
    "@types/jest": "^27.0.3",
    "@types/node": "^14.18.2",
    "@typescript-eslint/eslint-plugin": "^5.1.0",
    "@typescript-eslint/parser": "^5.1.0",
    "aws-cdk": "^2.8.0",
    "cross-env": "^7.0.3",
    "esbuild": "^0.14.7",
    "esbuild-jest": "^0.5.0",
    "eslint": "^8.0.1",
    "jest": "^27.4.5",
    "moment-timezone": "^0.5.33",
    "npm-run-all": "^4.1.5",
    "ts-node": "^9.0.0",
    "typescript": "^4.5.4"
  },
  "dependencies": {
    "@aws-sdk/client-codebuild": "^3.45.0",
    "@aws-sdk/client-cognito-identity-provider": "^3.36.1",
    "@aws-sdk/client-secrets-manager": "^3.45.0",
    "@aws-sdk/credential-providers": "^3.36.1",
    "@aws-sdk/middleware-retry": "^3.39.0",
    "ajv": "^8.6.3",
    "aws-cdk-lib": "^2.4.0",
    "aws-jwt-verify": "^1.0.3",
    "constructs": "^10.0.0",
    "source-map-support": "^0.5.16"
  }
}
