{
  "name": "noble-aesthetic",
  "version": "0.1.0",
  "description": "The custom, full stack noble aesthetic website.",
  "scripts": {
    "aws-token": "npm run ts-node bin/awsToken",
    "build": "run-s clean build:*",
    "build:client": "npm run ts-node bin/buildClient",
    "clean": "cross-env-shell \"rm -rf cdk.out bin/*.js coverage dist lib/*.js models/*.js src/**/*.js stacks/*.js test/*.js config.js\" && mkdir dist",
    "cdk": "cdk",
    "deploy": "run-s build deploy:server build:client deploy:resources",
    "deploy:api": "npm run ts-node bin/deployApi",
    "deploy:client": "npm run ts-node bin/deployClient",
    "deploy:resources": "run-p deploy:api deploy:client",
    "deploy:server": "npm run ts-node bin/deploy",
    "destroy": "npm run ts-node bin/destroy",
    "dev": "cd client && npm run dev",
    "diff": "npm run cdk diff",
    "hotswap": "npm run ts-node bin/hotswap",
    "lint": "eslint . --ext .ts",
    "postinstall": "cd client && npm i",
    "pr-check": "run-p lint synth test",
    "setClientEnv": "npm run ts-node bin/setClientEnv",
    "synth": "run-s build \"ts-node bin/synth\"",
    "test": "jest --coverage --passWithNoTests",
    "ts-node": "ts-node",
    "tsc": "cross-env-shell \"node_modules/.bin/tsc --project tsconfig.build.json\"",
    "watch": "npm run ts-node bin/cdkWatch"
  },
  "devDependencies": {
    "@aws-sdk/client-cloudfront": "^3.53.0",
    "@aws-sdk/client-cloudwatch-logs": "^3.52.0",
    "@aws-sdk/client-iam": "^3.43.0",
    "@aws-sdk/client-sts": "^3.43.0",
    "@aws-sdk/credential-provider-ini": "^3.41.0",
    "@types/aws-lambda": "^8.10.88",
    "@types/eslint": "^8.4.1",
    "@types/jest": "^27.0.3",
    "@types/luxon": "^2.3.1",
    "@types/mime-types": "^2.1.1",
    "@types/node": "^14.18.2",
    "@typescript-eslint/eslint-plugin": "^5.1.0",
    "@typescript-eslint/parser": "^5.1.0",
    "aws-cdk": "^2.15.0",
    "aws-sdk-client-mock": "^0.6.0",
    "cross-env": "^7.0.3",
    "esbuild": "^0.14.7",
    "esbuild-jest": "^0.5.0",
    "eslint": "^8.0.1",
    "jest": "^27.4.5",
    "jest-mock-process": "^1.4.1",
    "mime-types": "^2.1.34",
    "moment-timezone": "^0.5.33",
    "npm-run-all": "^4.1.5",
    "ts-jest": "^27.1.3",
    "ts-node": "^9.0.0",
    "typescript": "^4.5.4"
  },
  "dependencies": {
    "@aws-sdk/client-codebuild": "^3.45.0",
    "@aws-sdk/client-cognito-identity-provider": "^3.36.1",
    "@aws-sdk/client-dynamodb": "^3.51.0",
    "@aws-sdk/client-secrets-manager": "^3.45.0",
    "@aws-sdk/credential-providers": "^3.36.1",
    "@aws-sdk/lib-dynamodb": "^3.51.0",
    "@aws-sdk/middleware-retry": "^3.39.0",
    "ajv": "^8.6.3",
    "aws-cdk-lib": "^2.4.0",
    "aws-jwt-verify": "^1.0.3",
    "constructs": "^10.0.0",
    "luxon": "^2.3.1",
    "s3-sync-client": "^3.0.0",
    "source-map-support": "^0.5.16",
    "winston": "^3.6.0"
  }
}
